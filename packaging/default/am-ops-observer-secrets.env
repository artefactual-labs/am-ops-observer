# /etc/am-ops-observer/secrets.env
#
# Secrets file for am-ops-observer.
#
# In packaged deployments this file is loaded by systemd LoadCredential and
# exposed to the process through APP_SECRETS_FILE in a private runtime path.
#
# Recommended ownership/permissions:
#   owner: root
#   mode:  0600

# MCP MySQL password.
# Required only when APP_DB_ENABLED=true.
APP_DB_PASSWORD="change-me"

# Storage Service MySQL password.
# Required only when APP_SS_DB_ENABLED=true.
APP_SS_DB_PASSWORD="change-me"
